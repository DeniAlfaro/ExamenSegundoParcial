<UserControl x:Class="ExamenSegundoParcial.Contacto"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExamenSegundoParcial"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="White">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0">Correo:</TextBlock>
        <TextBox Grid.Row="1" Name="correoC"></TextBox>
        <TextBlock Grid.Row="2">Telefono:</TextBlock>
        <TextBox Grid.Row="3" Name="telefonoC"></TextBox>
        <TextBlock Grid.Row="4">Celular:</TextBlock>
        <TextBox Grid.Row="5" Name="celularC"></TextBox>
        <TextBlock Name="error2" Grid.Row="6" HorizontalAlignment="Left" Visibility="Hidden">Es necesario llenar todos los campos</TextBlock>
    </Grid>
</UserControl>
